<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" xmlns:comp="http://www.sbml.org/sbml/level3/version1/comp/version1" level="3" version="2" comp:required="true">
  <notes>
    <body xmlns="http://www.w3.org/1999/xhtml">
      <p>Created with <a href="https://github.com/matthiaskoenig/sbmlutils">https://github.com/matthiaskoenig/sbmlutils</a>.
<a href="https://doi.org/10.5281/zenodo.5525390">
        <img src="https://zenodo.org/badge/DOI/10.5281/zenodo.5525390.svg" alt="DOI"/></a></p>
      </body>
    </notes>
  <model id="model2" name="Model 2">
    <listOfCompartments>
      <compartment id="Vgut" name="gut compartment" spatialDimensions="3" size="1" constant="true"/>
      <compartment id="Vperi" name="peripheral compartment" spatialDimensions="3" size="1" constant="true"/>
      <compartment id="Vcent" name="central compartment" spatialDimensions="3" size="1" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="y_gut" name="y gut" compartment="Vgut" initialAmount="1" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>handled in amount not concentration</p>
          </body>
        </notes>
      </species>
      <species id="y_cent" name="y central" compartment="Vcent" initialConcentration="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="y_peri" name="y peripheral" compartment="Vperi" initialConcentration="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="k" value="1" constant="true"/>
      <parameter id="CL" value="1" constant="true"/>
      <parameter id="Q" value="1" constant="true"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="ABSORPTION" name="absorption" reversible="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>absorption from gut</p>
          </body>
        </notes>
        <listOfReactants>
          <speciesReference species="y_gut" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="y_cent" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <apply>
                <minus/>
                <ci> k </ci>
              </apply>
              <ci> y_gut </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="CLEARANCE" name="clearance" reversible="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>clearance from central compartment</p>
          </body>
        </notes>
        <listOfReactants>
          <speciesReference species="y_cent" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <apply>
                <minus/>
                <ci> CL </ci>
              </apply>
              <ci> y_cent </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R1" reversible="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>distribution in peripheral compartment</p>
          </body>
        </notes>
        <listOfReactants>
          <speciesReference species="y_cent" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="y_peri" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Q </ci>
              <ci> y_cent </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R2" reversible="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>distribution from peripheral compartment</p>
          </body>
        </notes>
        <listOfReactants>
          <speciesReference species="y_peri" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="y_cent" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Q </ci>
              <ci> y_peri </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
